{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions } from \"vuex\";\nimport AddPrd from \"./addPrd.vue\";\nimport editPrd from \"./editPrd.vue\";\nexport default {\n  name: \"Categorias\",\n  components: {\n    AddPrd,\n    editPrd\n  },\n  data() {\n    return {\n      status: \"\",\n      message: \"\",\n      item: \"\",\n      search_value: \"\",\n      updsearchcli: \"\",\n      currentPage: 1,\n      itemsPerPage: 12,\n      active: false,\n      viewModal: false,\n      viewModalDue: false,\n      itemDel: \"\",\n      radius: false\n    };\n  },\n  created() {\n    this.getAllInfoPrd(\"product\");\n  },\n  computed: {\n    data() {\n      let arr = this.$store.getters[\"product/data\"];\n      let filter = [];\n      if (!Array.isArray(arr)) {\n        return [];\n      } else {\n        if (this.search_value != \"\") {\n          for (const item of arr) {\n            if (this.includesItem(this.search_value, item.name)) {\n              filter.push(item);\n            }\n          }\n        } else {\n          filter = arr;\n        }\n      }\n      return filter;\n    },\n    historyOption() {\n      return this.$store.getters[\"product/getHistoryOption\"];\n    },\n    //Paginacion\n    allItems() {\n      return Object.keys(this.data).map(pid => this.data[pid]);\n    },\n    filteredItems() {\n      return this.allItems.slice((this.currentPage - 1) * this.itemsPerPage, this.currentPage * this.itemsPerPage);\n    },\n    totalPages() {\n      let total = Math.ceil(this.allItems.length / this.itemsPerPage);\n      if (total < this.currentPage) {\n        this.currentPage = 1;\n      }\n      return total;\n    }\n  },\n  methods: {\n    ...mapActions(\"product\", [\"getAllInfoPrd\"]),\n    ...mapActions(\"product\", [\"setHistoryOptionPrd\"]),\n    ...mapActions(\"product\", [\"setAddedPrd\"]),\n    ...mapActions(\"product\", [\"getInfoByIdPrd\"]),\n    changeView(option, item) {\n      this.item = item;\n      this.setAddedPrd(item);\n      this.setHistoryOptionPrd(option);\n    },\n    includesItem(search, name) {\n      if (search == \"\") {\n        return true;\n      }\n      name = name.toLowerCase();\n      search = search.toLowerCase();\n      if (search == \"\") {\n        return false;\n      }\n      return name.includes(search);\n    },\n    activeRadius() {\n      this.radius ? this.radius = false : this.radius = true;\n    },\n    showModal: function (id) {\n      this.itemDel = id;\n      this.viewModal = true;\n    },\n    closeModal: function () {\n      this.viewModal = false;\n      this.itemDel = \"\";\n    },\n    wait: function () {\n      setTimeout(() => this.$router.go(), 200);\n    },\n    delStatus: function () {\n      setTimeout(() => this.delMsgs(), 2000);\n      setTimeout(() => this.wait(), 500);\n    },\n    delMsgs: function () {\n      this.status = \"\";\n      this.message = \"\";\n    },\n    toAddOther: async function (name, id_category, descr, price, image, extras, subcat, more_pro) {\n      this.status = \"\";\n      this.message = \"\";\n      let data = [];\n      extras = JSON.stringify(extras);\n      subcat = JSON.stringify(subcat);\n      let p = price;\n      if (more_pro == \"SI\") {\n        p = 1;\n      }\n      if (image == null) {\n        data = {\n          name: name,\n          id_category: id_category,\n          descr: descr,\n          price: p,\n          extras,\n          subcat,\n          more_pro\n        };\n      } else {\n        data = new FormData();\n        data.append(\"name\", name);\n        data.append(\"id_category\", id_category);\n        data.append(\"descr\", descr);\n        data.append(\"price\", p);\n        data.append(\"image\", image);\n        data.append(\"subcat\", subcat);\n        data.append(\"extras\", extras);\n        data.append(\"more_pro\", more_pro);\n        data.append(\"_method\", \"POST\");\n      }\n      let result = await this.$store.dispatch(\"product/addItemPrd\", {\n        option: \"product\",\n        item: data\n      });\n      console.log(result);\n      if (result.status == \"error\") {\n        this.status = \"error\";\n        this.message = result.message;\n      } else {\n        // success\n        let payload = {\n          id: result.added.id,\n          option: \"product\"\n        };\n        let arrl = await this.getInfoByIdPrd(payload);\n        this.setAddedPrd(arrl);\n        this.changeView(\"Edit\", result.added);\n        this.viewModalDue = true;\n        window.scrollTo(0, 0);\n      }\n    },\n    continuar() {\n      this.viewModalDue = false;\n    },\n    eliminar: async function () {\n      let id = this.itemDel;\n      this.status = \"\";\n      this.message = \"\";\n      let result = await this.$store.dispatch(\"product/deleteItemPrd\", {\n        option: \"product\",\n        id: id\n      });\n      if (result.status == \"error\") {\n        this.message = result.message;\n        this.status = \"error\";\n        this.delStatus();\n      } else {\n        this.status = \"\";\n        this.message = \"\";\n        this.wait();\n      }\n    },\n    // botones de paginacion.\n    next() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prev() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AAuPA;AACA;AACA;AACA;EACAA;EACAC;IACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;EACA;EAEAC;IACAd;MACA;MACA;MACA;QACA;MACA;QACA;UACA;YACA;cACAe;YACA;UACA;QACA;UACAA;QACA;MACA;MAEA;IACA;IACAC;MACA;IACA;IAEA;IACAC;MACA;IACA;IACAC;MACA,2BACA,4CACA,qCACA;IACA;IACAC;MACA;MACA;QACA;MACA;MACA;IACA;EACA;EACAC;IACA;IACA;IACA;IACA;IAEAC;MACA;MACA;MACA;IACA;IAEAC;MACA;QACA;MACA;MACA1B;MACA2B;MACA;QACA;MACA;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;MACA;IACA;IACAC;MACA;MACA;IACA;IAEAC;MACAC;IACA;IAEAC;MACAD;MACAA;IACA;IACAE;MACA;MACA;IACA;IAEAC,4BACAnC,MACAoC,aACAC,OACAC,OACAC,OACAC,QACAC,QACAC,UACA;MACA;MACA;MACA;MACAF;MACAC;MACA;MACA;QACAE;MACA;MAEA;QACAvC;UACAJ;UACAoC;UACAC;UACAC;UACAE;UACAC;UACAC;QACA;MACA;QACAtC;QACAA;QACAA;QACAA;QACAA;QACAA;QACAA;QACAA;QACAA;QACAA;MACA;MACA;QACAwC;QACArC;MACA;MACAsC;MAEA;QACA;QACA;MACA;QACA;QACA;UACAC;UACAF;QACA;QACA;QACA;QACA;QACA;QACAG;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;MACA;MACA;MAEA;QACAL;QACAE;MACA;MAEA;QACA;QACA;QACA;MACA;QACA;QACA;QACA;MACA;IACA;IAEA;IACAI;MACA;QACA;MACA;IACA;IACAC;MACA;QACA;MACA;IACA;EACA;AACA","names":["name","components","AddPrd","editPrd","data","status","message","item","search_value","updsearchcli","currentPage","itemsPerPage","active","viewModal","viewModalDue","itemDel","radius","created","computed","filter","historyOption","allItems","filteredItems","totalPages","methods","changeView","includesItem","search","activeRadius","showModal","closeModal","wait","setTimeout","delStatus","delMsgs","toAddOther","id_category","descr","price","image","extras","subcat","more_pro","p","option","console","id","window","continuar","eliminar","next","prev"],"sourceRoot":"src/views/products","sources":["productos.vue"],"sourcesContent":["<template>\r\n  <div id=\"Categorias\">\r\n    <div v-if=\"historyOption == 'Default'\">\r\n      <div class=\"dflx\">\r\n        <p class=\"noitems\">\r\n          No. de items <span v-if=\"data.length > 0\">{{ data.length }}</span>\r\n          <span v-else>0</span>\r\n        </p>\r\n        <div class=\"select-container\">\r\n          <input\r\n            type=\"checkbox\"\r\n            name=\"select-ch\"\r\n            id=\"select-ch\"\r\n            v-model=\"active\"\r\n            true-value=\"true\"\r\n            false-value=\"false\"\r\n          />\r\n          <label for=\"select-ch\">\r\n            <div id=\"select-items\" :class=\"{ rounded: active }\">\r\n              Items por página: {{ itemsPerPage }}\r\n            </div>\r\n          </label>\r\n          <div class=\"options-cont\" @click=\"closeSelect()\">\r\n            <div\r\n              class=\"option-btn\"\r\n              v-bind:value=\"6\"\r\n              @click=\"itemsPerPage = '6'\"\r\n            >\r\n              6\r\n            </div>\r\n            <div\r\n              class=\"option-btn\"\r\n              v-bind:value=\"12\"\r\n              @click=\"itemsPerPage = '12'\"\r\n            >\r\n              12\r\n            </div>\r\n            <div\r\n              class=\"option-btn\"\r\n              v-bind:value=\"18\"\r\n              @click=\"itemsPerPage = '18'\"\r\n            >\r\n              18\r\n            </div>\r\n            <div\r\n              class=\"option-btn\"\r\n              v-bind:value=\"24\"\r\n              @click=\"itemsPerPage = '24'\"\r\n            >\r\n              24\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"buscador dflx\">\r\n          <input\r\n            v-model=\"search_value\"\r\n            type=\"search\"\r\n            id=\"input-search\"\r\n            class=\"form-control\"\r\n            :placeholder=\"'Búsqueda'\"\r\n          />\r\n          <div class=\"icon_buscar\">\r\n            <img src=\"../../assets/default/Search.png\" alt=\"icono buscar\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div @click=\"changeView('Add', '')\" class=\"btn_añadir dflx\">\r\n        <p>AGREGAR</p>\r\n        <img src=\"../../assets/default/add.png\" alt=\"\" />\r\n      </div>\r\n\r\n      <div class=\"tbl_categorias\">\r\n        <div class=\"tbl_header dflx\">\r\n          <div class=\"tblh tblrad name\">\r\n            <p>NOMBRE</p>\r\n          </div>\r\n          <div class=\"tblh\">\r\n            <p>CATEGORIA / SUBCAT.</p>\r\n          </div>\r\n          <div class=\"tblh\">\r\n            <p>DESCRIPCIÓN</p>\r\n          </div>\r\n          <div class=\"tblh\">\r\n            <p>ESTADO</p>\r\n          </div>\r\n          <div class=\"tblh\">\r\n            <p>PRECIO</p>\r\n          </div>\r\n\r\n          <div class=\"tblh2\">\r\n            <p>\r\n              AGREGAR <br />\r\n              OTRO\r\n            </p>\r\n          </div>\r\n          <div class=\"tblh2\">\r\n            <p>EDITAR</p>\r\n          </div>\r\n          <div class=\"tblh2 tblh2r\">\r\n            <p>BORRAR</p>\r\n          </div>\r\n        </div>\r\n        <div class=\"tbl_content\">\r\n          <div v-if=\"data.length > 0\">\r\n            <div\r\n              class=\"tbl_items dflx\"\r\n              v-for=\"(item, key) in filteredItems\"\r\n              :key=\"key\"\r\n            >\r\n              <div class=\"tbl_item name\">\r\n                <p>\r\n                  {{ item.name }}\r\n                  <span v-if=\"item.product_atributo_valores.length > 0\">{{\r\n                    \" - \" +\r\n                    item.product_atributo_valores[0].valor +\r\n                    \" \" +\r\n                    item.product_atributo_valores[0].atributo\r\n                  }}</span>\r\n                </p>\r\n              </div>\r\n              <div class=\"tbl_item\">\r\n                <p>\r\n                  {{ item.category_name }}\r\n                  <span v-if=\"item.subcategorias[0]\"\r\n                    >/ {{ item.subcategorias[0].subcategory_name }}</span\r\n                  >\r\n                </p>\r\n              </div>\r\n              <div class=\"tbl_item\">\r\n                <p>{{ item.descr }}</p>\r\n              </div>\r\n              <div class=\"tbl_item\">\r\n                <p>{{ item.status }}</p>\r\n              </div>\r\n              <div class=\"tbl_item\">\r\n                <p>{{ \"$\" + item.price }}</p>\r\n              </div>\r\n\r\n              <div class=\"icon_editar\">\r\n                <img\r\n                  v-if=\"item.more_pro == 'SI'\"\r\n                  @click=\"\r\n                    toAddOther(\r\n                      item.name,\r\n                      item.id_category,\r\n                      item.descr,\r\n                      item.price,\r\n                      item.image,\r\n                      item.extras,\r\n                      item.subcategorias,\r\n                      item.more_pro\r\n                    )\r\n                  \"\r\n                  src=\"../../assets/default/i_agregar.png\"\r\n                  alt=\"icono editar\"\r\n                />\r\n              </div>\r\n              <div class=\"icon_editar\">\r\n                <img\r\n                  @click=\"changeView('Edit', item)\"\r\n                  src=\"../../assets/default/i_editar.png\"\r\n                  alt=\"icono editar\"\r\n                />\r\n              </div>\r\n              <div class=\"icon_eliminar\">\r\n                <img\r\n                  @click=\"showModal(item.id)\"\r\n                  src=\"../../assets/default/i_eliminar.png\"\r\n                  alt=\"icono eliminar\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div v-else>\r\n            <p class=\"snu\">Aun no hay registros.</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div id=\"navigate-buttons\" class=\"mlf\">\r\n        <div class=\"dflx\">\r\n          <button @click=\"prev\" id=\"btn-prev\" class=\"btn-navigate\">\r\n            <img src=\"../../assets/default/Left.png\" alt=\"icono anterior\" />\r\n          </button>\r\n          <p class=\"num-pages\">{{ currentPage }} / {{ totalPages }}</p>\r\n          <button @click=\"next\" id=\"btn-next\" class=\"btn-navigate\">\r\n            <img src=\"../../assets/default/Right.png\" alt=\"icono siguiente\" />\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div v-if=\"historyOption == 'Add'\">\r\n      <!-- Agregar categorias -->\r\n      <AddPrd />\r\n    </div>\r\n    <div v-if=\"historyOption == 'Edit'\">\r\n      <!-- Editar categorias -->\r\n      <editPrd />\r\n    </div>\r\n\r\n    <div v-if=\"status != ''\" class=\"status_messages\">\r\n      <div v-if=\"status == 'success'\" class=\"msg msg_success\">\r\n        <p>{{ message }}</p>\r\n      </div>\r\n      <div v-if=\"status == 'error'\" class=\"msg msg_error\">\r\n        <p>{{ message }}</p>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-if=\"viewModal\" class=\"modal_view\">\r\n      <div id=\"modal_delete\">\r\n        <div class=\"body\">\r\n          <img src=\"../../assets/default/alert.png\" alt=\"icono alerta\" />\r\n          <h3>\r\n            ¿Estás seguro de\r\n            <span\r\n              >eliminar <br />\r\n              este producto?</span\r\n            >\r\n          </h3>\r\n        </div>\r\n\r\n        <div class=\"modal_del_btns dflx\">\r\n          <p @click=\"eliminar()\">Aceptar</p>\r\n          <p @click=\"closeModal()\">Cancelar</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-if=\"viewModalDue\" class=\"modal_view\">\r\n      <div id=\"modal_edit\">\r\n        <div class=\"body dflx\">\r\n          <h3>Producto agregado con éxito<br /><br /></h3>\r\n          <img src=\"../../assets/default/add.png\" alt=\"icono alerta\" />\r\n        </div>\r\n\r\n        <div class=\"modal_edit_btns dflx\">\r\n          <p @click=\"continuar()\" class=\"inicio\">Continuar</p>\r\n          <!---->\r\n          <!--<p @click=\"wait()\" class=\"otro\">Aceptar</p>-->\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapActions } from \"vuex\";\r\nimport AddPrd from \"./addPrd.vue\";\r\nimport editPrd from \"./editPrd.vue\";\r\nexport default {\r\n  name: \"Categorias\",\r\n  components: {\r\n    AddPrd,\r\n    editPrd,\r\n  },\r\n  data() {\r\n    return {\r\n      status: \"\",\r\n      message: \"\",\r\n      item: \"\",\r\n      search_value: \"\",\r\n      updsearchcli: \"\",\r\n      currentPage: 1,\r\n      itemsPerPage: 12,\r\n      active: false,\r\n      viewModal: false,\r\n      viewModalDue: false,\r\n      itemDel: \"\",\r\n      radius: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.getAllInfoPrd(\"product\");\r\n  },\r\n\r\n  computed: {\r\n    data() {\r\n      let arr = this.$store.getters[\"product/data\"];\r\n      let filter = [];\r\n      if (!Array.isArray(arr)) {\r\n        return [];\r\n      } else {\r\n        if (this.search_value != \"\") {\r\n          for (const item of arr) {\r\n            if (this.includesItem(this.search_value, item.name)) {\r\n              filter.push(item);\r\n            }\r\n          }\r\n        } else {\r\n          filter = arr;\r\n        }\r\n      }\r\n\r\n      return filter;\r\n    },\r\n    historyOption() {\r\n      return this.$store.getters[\"product/getHistoryOption\"];\r\n    },\r\n\r\n    //Paginacion\r\n    allItems() {\r\n      return Object.keys(this.data).map((pid) => this.data[pid]);\r\n    },\r\n    filteredItems() {\r\n      return this.allItems.slice(\r\n        (this.currentPage - 1) * this.itemsPerPage,\r\n        this.currentPage * this.itemsPerPage\r\n      );\r\n    },\r\n    totalPages() {\r\n      let total = Math.ceil(this.allItems.length / this.itemsPerPage);\r\n      if (total < this.currentPage) {\r\n        this.currentPage = 1;\r\n      }\r\n      return total;\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapActions(\"product\", [\"getAllInfoPrd\"]),\r\n    ...mapActions(\"product\", [\"setHistoryOptionPrd\"]),\r\n    ...mapActions(\"product\", [\"setAddedPrd\"]),\r\n    ...mapActions(\"product\", [\"getInfoByIdPrd\"]),\r\n\r\n    changeView(option, item) {\r\n      this.item = item;\r\n      this.setAddedPrd(item);\r\n      this.setHistoryOptionPrd(option);\r\n    },\r\n\r\n    includesItem(search, name) {\r\n      if (search == \"\") {\r\n        return true;\r\n      }\r\n      name = name.toLowerCase();\r\n      search = search.toLowerCase();\r\n      if (search == \"\") {\r\n        return false;\r\n      }\r\n      return name.includes(search);\r\n    },\r\n\r\n    activeRadius() {\r\n      this.radius ? (this.radius = false) : (this.radius = true);\r\n    },\r\n\r\n    showModal: function (id) {\r\n      this.itemDel = id;\r\n      this.viewModal = true;\r\n    },\r\n    closeModal: function () {\r\n      this.viewModal = false;\r\n      this.itemDel = \"\";\r\n    },\r\n\r\n    wait: function () {\r\n      setTimeout(() => this.$router.go(), 200);\r\n    },\r\n\r\n    delStatus: function () {\r\n      setTimeout(() => this.delMsgs(), 2000);\r\n      setTimeout(() => this.wait(), 500);\r\n    },\r\n    delMsgs: function () {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n    },\r\n\r\n    toAddOther: async function (\r\n      name,\r\n      id_category,\r\n      descr,\r\n      price,\r\n      image,\r\n      extras,\r\n      subcat,\r\n      more_pro\r\n    ) {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n      let data = [];\r\n      extras = JSON.stringify(extras);\r\n      subcat = JSON.stringify(subcat);\r\n      let p = price;\r\n      if (more_pro == \"SI\") {\r\n        p = 1;\r\n      }\r\n\r\n      if (image == null) {\r\n        data = {\r\n          name: name,\r\n          id_category: id_category,\r\n          descr: descr,\r\n          price: p,\r\n          extras,\r\n          subcat,\r\n          more_pro,\r\n        };\r\n      } else {\r\n        data = new FormData();\r\n        data.append(\"name\", name);\r\n        data.append(\"id_category\", id_category);\r\n        data.append(\"descr\", descr);\r\n        data.append(\"price\", p);\r\n        data.append(\"image\", image);\r\n        data.append(\"subcat\", subcat);\r\n        data.append(\"extras\", extras);\r\n        data.append(\"more_pro\", more_pro);\r\n        data.append(\"_method\", \"POST\");\r\n      }\r\n      let result = await this.$store.dispatch(\"product/addItemPrd\", {\r\n        option: \"product\",\r\n        item: data,\r\n      });\r\n      console.log(result);\r\n\r\n      if (result.status == \"error\") {\r\n        this.status = \"error\";\r\n        this.message = result.message;\r\n      } else {\r\n        // success\r\n        let payload = {\r\n          id: result.added.id,\r\n          option: \"product\",\r\n        };\r\n        let arrl = await this.getInfoByIdPrd(payload);\r\n        this.setAddedPrd(arrl); \r\n        this.changeView(\"Edit\", result.added);\r\n        this.viewModalDue = true;\r\n        window.scrollTo(0, 0);\r\n      }\r\n    },\r\n\r\n    continuar() {\r\n      this.viewModalDue = false;\r\n    },\r\n\r\n    eliminar: async function () {\r\n      let id = this.itemDel;\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n\r\n      let result = await this.$store.dispatch(\"product/deleteItemPrd\", {\r\n        option: \"product\",\r\n        id: id,\r\n      });\r\n\r\n      if (result.status == \"error\") {\r\n        this.message = result.message;\r\n        this.status = \"error\";\r\n        this.delStatus();\r\n      } else {\r\n        this.status = \"\";\r\n        this.message = \"\";\r\n        this.wait();\r\n      }\r\n    },\r\n\r\n    // botones de paginacion.\r\n    next() {\r\n      if (this.currentPage < this.totalPages) {\r\n        this.currentPage++;\r\n      }\r\n    },\r\n    prev() {\r\n      if (this.currentPage > 1) {\r\n        this.currentPage--;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\np.three-dots {\r\n  text-overflow: ellipsis;\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n}\r\n.tbl_categorias {\r\n  margin-top: 1.9791666666666665vw;\r\n  width: 81%;\r\n}\r\n\r\n.tbl_items {\r\n  border-bottom: 1px solid var(--color-4);\r\n}\r\n\r\n.tbl_item {\r\n  overflow: auto;\r\n}\r\n.tbl_item.name,\r\n.tblh.name {\r\n  width: 15vw;\r\n}\r\n.tblh {\r\n  width: 10.78125vw;\r\n  height: 2.5vw;\r\n  background: var(--color-2);\r\n  border-right: 1px solid var(--color-5);\r\n}\r\n.tblh2 {\r\n  width: 2.5vw;\r\n  height: 2.5vw;\r\n  background: var(--color-3);